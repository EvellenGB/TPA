package programaParImpar;
import java.util.Scanner;
public class programaParImpar {
	public static void main(String[]args) {
		Scanner in=new Scanner(System.in);
		final int TAM=10;
		int i,a[],b[];
		
		a=new int [TAM];
		b=new int [TAM];
		
		for (i=0; i<TAM; i++) {
			System.out.println("Entre com o"+(i+1)+ " valor");
			a[i]= in.nextInt();
			b[i]= a[i]*a[i];
		}
		
		System.out.println();
		for (i=0; i<TAM;i++) {
			System.out.println("a raiz"+(i+1)+" quadrada  de b Ã©"+b[i]);
		}
	}

}
